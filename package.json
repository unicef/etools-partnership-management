    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "mocha": "^7.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.0",
    "prettier": "^2.0.5",
    "prpl-server": "^1.3.0",
    "typescript": "^3.9.5",
    "ua-parser-js": ">=0.7.24",
    "wct-mocha": "^1.1.0"
  },
  "husky": {
    "hooks": {
