{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "gulp serve",
    "build": "tsc && npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/app-route": "^3.0.2",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "etools-ajax": "git+https://github.com/unicef-polymer/etools-ajax#update-to-polymer3",
    "etools-app-selector": "git+https://github.com/unicef-polymer/etools-app-selector#upgrade-to-polymer3",
    "etools-behaviors": "git+https://github.com/unicef-polymer/etools-behaviors#upgrade-to-polymer3",
    "etools-dialog": "git+https://github.com/unicef-polymer/etools-dialog#upgrade-to-polymer3",
    "etools-dropdown": "git+https://github.com/unicef-polymer/etools-dropdown#upgrade-to-polymer3",
    "etools-loading": "git+https://github.com/unicef-polymer/etools-loading#upgrade-to-polymer3",
    "etools-profile-dropdown": "git+https://github.com/unicef-polymer/etools-profile-dropdown#upgrade-to-polymer3",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.2",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.3.0",
    "puppeteer": "^1.5.0",
    "typescript": "^3.1.3",
    "wct-mocha": "^1.0.0"
  }
}
