{
  "name": "pmp",
  "version": "0.0.0",
  "description": "eTools Partnership Management Portal",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-partnership-management"
  },
  "license": "https://github.com/unicef-polymer/etools-partnership-management/blob/master/LICENSE.md",
  "bugs": {
    "url": "https://github.com/unicef/etools-partnership-management/issues"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "dev": "polymer serve & npm run watch",
    "bundle": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp",
    "test": "polymer test",
    "watch": "browser-sync start --proxy localhost:8082 --files '**/*.html, **/*.js, images/*' "
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.23.6",
    "css-slam": "^1.1.0",
    "del": "^2.2.1",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^4.0.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-crisper": "^1.1.0",
    "gulp-csslint": "^1.0.1",
    "gulp-html-extract": "^0.2.0",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.4.0",
    "gulp-jscs": "^4.1.0",
    "gulp-jscs-stylish": "^1.4.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-polylint": "^1.0.3",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.4",
    "gulp-util": "^3.0.7",
    "htmlhint-stylish": "^1.0.3",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "lazypipe": "^1.0.1",
    "merge-stream": "^1.0.0",
    "plylog": "^0.4.0",
    "polymer-build": "^0.4.0",
    "web-component-tester": "^6.5.0",
    "yargs": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "redux": "^3.6.0",
    "uglify-js-harmony": "^2.7.5"
  }
}
