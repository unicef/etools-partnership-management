{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2018", "dom", "esnext"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "outDir": "src",
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "src_ts/**/*.ts",
    "src_ts/components/app-modules/interventions/pages/intervention-tab-pages/assets/i18n/en.json",
    "src_ts/components/app-modules/interventions/pages/intervention-tab-pages/assets/i18n/ro.json",
    "src_ts/components/app-modules/interventions/pages/intervention-tab-pages/assets/i18n/ar.json"
  ],
  "exclude": []
}
